#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

void check (int x, vector<int> &y) {
    for (int i = 0; i < y.size(); i++) {
        if (y[i] == x) {
            y.erase(y.begin() + i);
            break;
        }
    }
}

int main ()
{
    int n, a, b, c, x;
    while (cin >> n >> a >> b >> c >> x, (n || a || b || c || x)) {
        int count = 0;
        for (int i = 0; i < n; i++) {
            int y;
            cin >> y;
            while (count < 10002) {
                if (y == x) {
                    break;
                } else {
                    x = (a * x + b) % c;
                    count++;
                }
            }
            x = (a * x + b) % c;
            count++;
        }
        cout << (count <= 10000 ? count : -1) << endl;
    }

    return 0;
}
