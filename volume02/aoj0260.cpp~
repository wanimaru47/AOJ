#include <iostream>
#include <vector>
using namespace std;

int main ()
{
    int n;
    while (cin >> n, n) {
        vector<int> p;
        vector<int> j;

        int t;
        for (int i = 0; i < n; i++, cin >> t) p.push_back(t);
        for (int i = 0; i < n - 1; i++, cin >> t) j.push_back(t);
    }

    return 0;
}
