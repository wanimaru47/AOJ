import std.stdio;
import std.string;
import std.conv;

int main()
{
    int n;
    while (cin >> n, n > 0) {
        int[][] num;
        for (int i = 0; i < n; i++) {
            string[] str = readln().chomp().split();
            for (int j = 0; j < n; j++)
                num[i][j] = str[j].to!int();
        }
    }

    return 0;
}
