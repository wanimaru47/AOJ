import std.stdio;
import std.string;
import std.conv;

int main()
{
    int n = readln().chomp().to!int();
    string s = "Hoshino";
    while (n--) {
        string str = readln();
        for (int i = 0; i < str.length; i++) {
            if (str[i] == 'H') {
                bool flag = true;
                for (int j = 0; j < 7; j++) {
                    i += j;
                    if (str[i] != s[j]) flag = false;
                }
            }
        }
    }

    return 0;
}
