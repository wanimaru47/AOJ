#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main ()
{
    string str;
    while (cin >> str) {
        int cursor = 1;
        string res;
        for (int i = 0; i < str.size(); i += 3) {
            if (str.substr(i-2, 2) == "->") {
                res += str[i];
            } else {
                res = str[i] + res;
            }
        }

        cout << res << endl;
    }

    return 0;
}
